        /* Reduced motion */
        @media (prefers-reduced-motion: reduce) {
          .service-card,
          .services__badge,
          .services__title,
          .services__description,
          .services__cta,
          .service__feature {
            animation: none;
            opacity: 1;
            transform: none;
          }
        }
      </style>
    </section>
  `;

          // eslint-disable-next-line no-console
          console.log('üîç Axe-core accessibility monitoring enabled')
        } catch (error) {

          console.warn('Failed to initialize axe-core:', error)
        }
      }

      initAxe()
    }
  }, [])

  return null
}


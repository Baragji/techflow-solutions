    if (
      // eslint-disable-next-line no-restricted-globals
      typeof window !== 'undefined' &&
      process.env.NODE_ENV !== 'production'
    ) {
      const initAxe = async () => {
        try {
          const axe = await import('@axe-core/react')
          const ReactDOM = await import('react-dom')


  keywords?: string;
  image?: string;
  url?: string;
  type?: 'website' | 'article' | 'product';
  publishedTime?: string;
  modifiedTime?: string;
  author?: string;
  section?: string;
  tags?: string[];
}

const defaultConfig = {
  siteName: 'TechFlow Solutions',
  siteUrl: 'https://techflowsolutions.dk',
  defaultImage: '/images/og-default.jpg',
  twitterHandle: '@techflowdk',
  locale: 'da_DK',
  type: 'website' as const,

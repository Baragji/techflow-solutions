#!/usr/bin/env node

const fs = require('fs');
const path = require('path');

console.log('üîß Fixing Runtime Issues...\n');

// Color codes for console output
const colors = {
  red: '\x1b[31m',
  green: '\x1b[32m',
  yellow: '\x1b[33m',

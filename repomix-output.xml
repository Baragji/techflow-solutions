  border-radius: 8px;
  padding: 16px;
  will-change: transform, opacity;
  transition: all var(--anim-duration) var(--anim-easing);
}

/* Advanced layers - hidden by default */
.glass-layer-3,
.glass-layer-4 {
  opacity: 0;
  visibility: hidden;
  transition: opacity 0.3s ease-in-out, visibility 0.3s ease-in-out;
}

/* Feature flag activation */

'use client';

import { useHydration } from '@/hooks/useHydration';
import { LazyMotion, MotionProps, domAnimation, m } from 'framer-motion';
import { ElementType, ReactNode, createContext, useContext } from 'react';

// Context to track if LazyMotion is available
const LazyMotionContext = createContext(false);

// Provider component that wraps the app with LazyMotion
export const LazyMotionProvider = ({ children }: { children: ReactNode }) => (
  <LazyMotion features={domAnimation}>
    <LazyMotionContext.Provider value={true}>
      {children}
    </LazyMotionContext.Provider>
  </LazyMotion>
);

// Hook to check if LazyMotion is available
export const useLazyMotion = () => useContext(LazyMotionContext);

// Define motion component props
interface LazyMotionComponentProps extends Omit<MotionProps, 'children'> {
  children: ReactNode;
  as?: ElementType;
  fallbackClassName?: string;
  [key: string]: unknown;
}

/**
 * Optimized motion wrapper using LazyMotion with domAnimation
 * Provides SSR safety and bundle size optimization
 */
export const Motion = ({
  children,
  as = 'div',
  fallbackClassName = '',
  className = '',
  initial,
  animate,
  whileInView,
  whileHover,
  whileTap,
  transition,
  viewport,
  ...props
}: LazyMotionComponentProps) => {
  const isHydrated = useHydration();
  const hasLazyMotion = useLazyMotion();

  if (!isHydrated || !hasLazyMotion) {
    // During SSR and initial render, or without LazyMotion, use a regular element
    const StaticComponent = as as ElementType;
    return (
      <StaticComponent
        className={`${fallbackClassName} ${className}`.trim()}
        {...props}
      >
        {children}
      </StaticComponent>
    );
  }

  // Create the motion component using LazyMotion's 'm' export
  const MotionComponent = m[as as keyof typeof m] as ElementType;

  // After hydration with LazyMotion, use the optimized motion component
  return (
    <MotionComponent
      className={className}
      initial={initial}
      animate={animate}
      whileInView={whileInView}
      whileHover={whileHover}
      whileTap={whileTap}
      transition={transition}
      viewport={viewport}
      {...props}
    >
      {children}
    </MotionComponent>
  );
};

// Pre-built components for common use cases
export const MotionDiv = (props: LazyMotionComponentProps) => (
  <Motion as="div" {...props} />
);

export const MotionSection = (props: LazyMotionComponentProps) => (
  <Motion as="section" {...props} />
);

export const MotionArticle = (props: LazyMotionComponentProps) => (
  <Motion as="article" {...props} />
);

export const MotionForm = (props: LazyMotionComponentProps) => (
  <Motion as="form" {...props} />
);

export const MotionHeader = (props: LazyMotionComponentProps) => (
  <Motion as="header" {...props} />
);

export const MotionFooter = (props: LazyMotionComponentProps) => (
  <Motion as="footer" {...props} />
);

export const MotionNav = (props: LazyMotionComponentProps) => (
  <Motion as="nav" {...props} />
);

export const MotionButton = (props: LazyMotionComponentProps) => (
  <Motion as="button" {...props} />
);

export const MotionSpan = (props: LazyMotionComponentProps) => (
  <Motion as="span" {...props} />
);

export const MotionH1 = (props: LazyMotionComponentProps) => (
  <Motion as="h1" {...props} />
);

export const MotionH2 = (props: LazyMotionComponentProps) => (
  <Motion as="h2" {...props} />
);

export const MotionH3 = (props: LazyMotionComponentProps) => (
  <Motion as="h3" {...props} />
);

export const MotionP = (props: LazyMotionComponentProps) => (
  <Motion as="p" {...props} />
);

export const MotionUl = (props: LazyMotionComponentProps) => (
  <Motion as="ul" {...props} />
);

export const MotionLi = (props: LazyMotionComponentProps) => (
  <Motion as="li" {...props} />
);

// Animation presets for common patterns
export const fadeInUp = {
